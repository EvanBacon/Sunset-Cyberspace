{
  expo: {
    name: "Sunset Cyberspace",
    description: "A retro runner game!",
    slug: "cyberspace",
    privacy: "unlisted",
    sdkVersion: "UNVERSIONED",
    version: "1.0.0",
    orientation: "landscape",
    scheme: "cyberspace",
    primaryColor: "#056ECF",
    icon: "./assets/expo-icon.png",
    splash: {
      image: "./assets/splash.png",
      resizeMode: "cover",
      backgroundColor: "#000000"
    },
    packagerOpts: {
      assetExts: [
        "ttf",
        "mp4",
        "obj",
        "jpg",
        "txt",
        "json"
      ],
      config: "./_universe/rn-cli.config.js"
    },
    ios: {
      supportsTablet: true,
      bundleIdentifier: "com.evanbacon.cyberspace",
      buildNumber: "0.0.0",
      icon: "./assets/icon.png",
      splash: {
        image: "./assets/splash.png",
        resizeMode: "cover",
        backgroundColor: "#000000",
        tabletImage: "./assets/splash.png"
      }
    },
    android: {},
    entryPoint: "_universe/entry.js"
  }
}